<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles.css">
    <title>Report</title>
    
</head>
<body>
    <div class="title">
        <h1>Filtered Results</h1>
        
    <!-- Table for View Inventory-->
    <div class="tableInventory">
        <table>
            <tr>
              <th>Date Bought</th>
              <th>Ingredients</th>
              <th>Unit Idontknow</th>
            </tr>

              <% order_report.forEach(order_report => { %>
                <tr>
                <td><%= order_report.dateBought %></td>
                <td><%= order_report.ingredientName %></td>
                <td><%=order_report.unit %></td>
              </tr>
               
            <% }) %>
          
            
          </table>

    </div>
    
</body>
</html>