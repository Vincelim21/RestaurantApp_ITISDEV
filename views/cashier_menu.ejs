<div class="inline-two">
    <div class="menu-container">
        <h1> MENU</h1>
        <div class="menu-card">

            <% recipename.forEach(recipename => { %>
                <tr>
                    <div class="card-container">
                    <td><%= recipename.recipeName %></td>
                    <p>PRICE</p>
                    </div>
                </tr>
            <% }); %> 
        </div>
    </div>
    <div class="checkout-container">
        <h1> CHECKOUT</h1>
        <div class="checkout-details">
            <table>
                <tr>
                    <th>ITEM NAME</th>
                    <th>QUANTITY</th>
                    <th>PRICE</th>
                </tr>
                <tr>
                    <td>ITEM X</td>
                    <td><input type="text"></td>
                    <td>PRICE</td>
                </tr>
                <tr>
                    <td>ITEM X</td>
                    <td><input type="text"></td>
                    <td>PRICE</td>
                </tr>

        </div>
        <div class="totals">
            <table>
                <tr>
                    <td>SUBTOTAL</td>
                    <td>PRICE</td>
                </tr>
                <tr>
                    <td>TOTAL</td>
                    <td>TOTAL</td>
                </tr>
        </div>
    </div>





</div>